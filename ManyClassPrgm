class Student{
	String name;
	int rollNo;
	String section;
	
	Student(String name,int rollNo,String section)
	{
	this.name = name;
	this.rollNo = rollNo;
	this.section = section;
	}
	
	public void printDetails()
	{
		System.out.println("Name :"+this.name+","+"Roll No :"+this.rollNo+","+"Section :"+this.section);
	}
}

public class mprg{

	public static void main(String[] args)
	{
		Student student1 = new Student("Vyshnavi",59,"MCA");
		Student student2 = new Student("hari",55,"Economics");
		Student student3 = new Student("Rohith",21,"Btech Civil");
		
		student1.printDetails();
		student2.printDetails();
		student3.printDetails();
	} 
	
}
